<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yasinta.kesehatankucing">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".utils.AppQu"
        android:allowBackup="true"
        android:icon="@drawable/icon_256"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_256"
        android:supportsRtl="true"
        android:allowClearUserData="true"
        android:theme="@style/Theme.MPASI.NoActionBar">
        <activity
            android:name=".activity.DetailJenisPenyakit"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/Theme.MPASI.NoActionBar" />
        <activity
            android:name=".activity.Splashscreen"
            android:exported="true"
            android:theme="@style/Theme.MPASI.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>